{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "Path": "Logs/CMSConfigLog-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "DatabaseSettings": {
    "DbProvider": "*****",
    "NpgSqlConnectionString": "Host=*****;Port=*****;Database=*****;Username=*****;Password=*****;Include Error Detail=true",
    "DockerConnectionString": "Host=*****;Port=*****;Database=*****;Username=*****;Password=*****;Include Error Detail=true",
    "SqlServerConnectionString": "Server=*****;Database=*****;Trusted_Connection=True;User ID=*****;Password=*****;MultipleActiveResultSets=true;Encrypt=False;"
  },
  "JwtSettings": {
    "Key": "**********",
    "Audience": "**********",
    "Issuer": "**********",
    "AccessTokenExpirationInMinutes": 0,
    "RefreshTokenExpirationInDays": 0
  },
  "MailSettings": {
    "Server": "*****",
    "Port": "***",
    "Username": "*********",
    "Password": "*************"
  }
}